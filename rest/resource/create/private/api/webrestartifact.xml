<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<restInterface>
    <method>POST</method>
    <path>/resource</path>
    <queryParameters>path, webApplicationId</queryParameters>
    <role>editor</role>
    <permissionContext>="webApplication:" + input/webApplicationId</permissionContext>
    <permissionAction>resource.write</permissionAction>
    <inputAsStream>true</inputAsStream>
    <outputAsStream>false</outputAsStream>
    <output>nabu.web.page.core.rest.resource.resource</output>
    <lenient>false</lenient>
    <webApplicationId>true</webApplicationId>
    <language>false</language>
    <allowFormBinding>false</allowFormBinding>
    <caseInsensitive>false</caseInsensitive>
    <cache>false</cache>
    <allowCookiesWithoutReferer>false</allowCookiesWithoutReferer>
    <allowCookiesWithExternalReferer>false</allowCookiesWithExternalReferer>
    <request>false</request>
</restInterface>
